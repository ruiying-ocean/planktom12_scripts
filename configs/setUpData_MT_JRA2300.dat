namelist_ref_jra_first_year:/gpfs/home/vhf24tbu/EXP00/namelist_ref_jra_first_year

# For bias runs we need the model to use forcing data that is a loop of a single year (currently set to 1990)
namelist_ref_era_looping:/gpfs/data/greenocean/software/CodeReview/KAR12/CONFIG/ORCA2_LIM_PlankTOM/EXP00/namelist_ref_era_looping
namelist_ref_jra_looping:/gpfs/home/vhf24tbu/EXP00/namelist_ref_jra_looping

# Short namelist of config namelist IDs
namelist_top_cfg:/gpfs/data/greenocean/software/CodeReview/KAR12/CONFIG/ORCA2_LIM_PlankTOM/EXP00/namelist_top_cfg

